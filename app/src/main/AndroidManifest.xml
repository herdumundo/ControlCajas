        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".registrar_repartidor"></activity>
        <activity android:name=".informe_registro_maples" />
        <activity android:name=".informe_menu" />
