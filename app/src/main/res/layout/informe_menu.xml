<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".informe_menu">

    <Button
        android:id="@+id/btn_cant_animales"
        android:layout_width="378dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="16dp"
        android:drawableLeft="@drawable/ic_action_reporte"
        android:onClick="ir_informe_maples"
        android:text="INFORME REGISTRO DE MAPLES"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="165dp" />

    <Button
        android:id="@+id/btn_detalle_registro"
        android:layout_width="379dp"
        android:layout_height="wrap_content"

        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="15dp"
        android:drawableLeft="@drawable/ic_action_lista_number"
        android:onClick="ir_informe_cajas"
        android:text="INFORME REGISTRO DE CAJAS"
        tools:ignore="MissingConstraints" />

</RelativeLayout>